<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Regression Suite" thread-count="10" parallel="classes">
	
	<listeners>
		<listener class-name="testComponents.Listeners"></listener>
	</listeners>
	
	<test name="RegressionTest">
		<parameter name="BrowserType" value="chrome"/>
		<classes>
			<class name="testRunners.Test_Runner_Parallel_1"></class>
			<class name="testRunners.Test_Runner_Parallel_2"></class>
		</classes>
	</test>
	
</suite>

